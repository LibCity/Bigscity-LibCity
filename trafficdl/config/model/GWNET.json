{
  "dropout": 0.3,
  "blocks": 4,
  "layers": 2,
  "gcn_bool": true,
  "addaptadj": true,
  "adjtype": "doubletransition",
  "randomadj": true,
  "aptonly": true,
  "kernel_size": 2,
  "nhid": 32,
  "residual_channels": 32,
  "dilation_channels": 32,
  "skip_channels": 256,
  "end_channels": 512,

  "scaler": "standard",
  "calculate_weight": true,
  "load_external": false,
  "normal_external": false,
  "add_time_in_day": false,
  "add_day_in_week": false,

  "max_epoch": 100,
  "learner": "adam",
  "learning_rate": 0.001,
  "lr_decay": false,
  "clip_grad_norm": true,
  "max_grad_norm": 5,
  "use_early_stop": false
}

{
  "gcn_true": true,
  "buildA_true": true,
  "gcn_depth": 2,
  "dropout": 0.3,
  "subgraph_size": 20,
  "node_dim": 40,
  "dilation_exponential": 1,
  "conv_channels": 32,
  "residual_channels": 32,
  "skip_channels": 64,
  "end_channels": 128,
  "layers": 3,
  "propalpha": 0.05,
  "tanhalpha": 3,
  "layer_norm_affline": true,
  "use_curriculum_learning": true,
  "step_size1": 2500,
  "task_level": 0,
  "num_split": 1,
  "step_size2": 100,

  "scaler": "standard",
  "load_external": true,
  "normal_external": false,
  "ext_scaler": "none",
  "add_time_in_day": true,
  "add_day_in_week": false,

  "max_epoch": 100,
  "learner": "adam",
  "learning_rate": 0.001,
  "weight_decay": 0.0001,
  "lr_decay": false,
  "clip_grad_norm": true,
  "max_grad_norm": 5,
  "use_early_stop": true,
  "patience": 50
}

{
  "embed_dim": 32,
  "p_interval": 1,
  "scaler": "minmax01",
  "load_external": false,
  "normal_external": false,
  "ext_scaler": "none",
  "add_time_in_day": true,
  "add_day_in_week": false,
  "use_row_column": true,

  "max_epoch": 100,
  "learner": "adam",
  "learning_rate": 0.001,
  "weight_decay": 1e-6,
  "lr_beta1": 0.9,
  "lr_beta2": 0.999,
  "lr_decay": false,
  "clip_grad_norm": false,
  "use_early_stop": true,
  "patience": 50,

  "lstm_hidden_dim" : 32,
  "kernel_type":"random_walk_diffusion",
  "cheby_order": 2,
  "loss" :"MSE"
}

{
  "cluster_nodes": 10,
  "nhid": 32,
  "dropout": 0.3,
  "channels": 32,
  "skip_channels": 256,
  "end_channels": 512,

  "scaler": "standard",
  "load_external": true,
  "normal_external": false,
  "ext_scaler": "none",
  "add_time_in_day": true,
  "add_day_in_week": false,

  "max_epoch": 100,
  "learner": "adam",
  "weight_decay": 0.0000,
  "learning_rate": 0.001,
  "lr_decay": true,
  "lr_scheduler": "exponentiallr",
  "lr_decay_ratio": 0.92,
  "use_early_stop": true,
  "patience": 50
}

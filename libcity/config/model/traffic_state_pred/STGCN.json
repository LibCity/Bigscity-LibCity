{
  "Ks": 3,
  "Kt": 3,
  "blocks": [[1, 32, 64], [64, 32, 128]],
  "dropout": 0,
  "graph_conv_type": "chebconv",
  "stgcn_train_mode": "quick",
  "bidir_adj_mx": true,

  "scaler": "standard",
  "load_external": false,
  "normal_external": false,
  "ext_scaler": "none",
  "add_time_in_day": false,

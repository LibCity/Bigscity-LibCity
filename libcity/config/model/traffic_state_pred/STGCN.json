  "max_epoch": 100,
  "learner": "rmsprop",
  "learning_rate": 0.001,
  "lr_decay": true,
  "lr_scheduler": "steplr",
  "lr_decay_ratio": 0.7,
  "step_size": 5,
  "clip_grad_norm": false,
  "use_early_stop": false
}

{
  "num_rnn_layers": 2,
  "rnn_units": 64,
  "mode": "refine",
  "nhid": 0,
  "nlayers": 1,
  "dropout_f": 0.1,
  "dropout_d": 0.1,
  "activation": "tanh",

  "scaler": "standard",
  "load_external": false,
  "normal_external": false,
  "ext_scaler": "none",
  "add_time_in_day": false,
  "add_day_in_week": false,

  "max_epoch": 100,
  "learner": "adam",
  "learning_rate": 0.01,
  "lr_decay": false,
  "clip_grad_norm": false,
  "use_early_stop": false
}

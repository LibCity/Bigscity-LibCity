{
  "batch_size": 32,
  "embed_size": 128,
  "hidden_size": 256,
  "input_window": 12,
  "output_window": 12,
  "channel_independence": true,
  "sparsity_threshold": 0.01,
  "scale": 0.02,

  "scaler": "standard",
  "load_external": false,

  "max_epoch": 200,
  "learner": "adam",
  "learning_rate": 0.0001,
  "lr_decay": false,
  "clip_grad_norm": false,
  "use_early_stop": true,
  "patience": 20
}

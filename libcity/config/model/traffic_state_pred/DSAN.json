{
  "L": 3,
  "d": 64,
  "n_h": 8,
  "B": 3,
  "l_d": 2,
  "r_d": 0.1,

  "scaler": "standard",
  "load_external": true,
  "normal_external": false,
  "ext_scaler": "none",
  "add_time_in_day": true,
  "add_day_in_week": true,

  "max_epoch": 100,
  "learner": "adam",
  "learning_rate": 0.0001,
  "lr_decay": true,
  "lr_epsilon": 1e-9,
  "clip_grad_norm": false,
  "use_early_stop": true,
  "patience": 50
}

{
    "input_dim": 4,
    "output_dim": 1,

    "encode_conv_dims": [32, 32, 128],
    "lstm_dims": [128, 64],
    "spatial_emb_dim": 90,
    "temporal_emb_dim": 30,

    "decode_conv_dims": [32, 32, 128],

    "batch_size": 16,
    "scaler": "minmax01",
    "load_external": true,
    "normal_external": false,
    "ext_scaler": "none",
    "use_row_column": true,

    "max_epoch": 100,
    "learner": "adam",
    "learning_rate": 0.001,
    "weight_decay": 1e-6,
    "lr_decay": false,
    "clip_grad_norm": false,
    "use_early_stop": true,
    "patience": 50
}